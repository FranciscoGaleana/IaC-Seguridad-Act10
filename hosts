[Ubuntu]
UbuntuServer ansible_host= ansible_user=ubuntu ansible_ssh_private_key_file=

[Windows]
WindowsServer ansible_host= ansible_user=Administrator ansible_ssh_private_key_file=

[Windows:vars]
ansible_user=Administrator
ansible_password=
ansible_connection=winrm
ansible_winrm_scheme=https
ansible_winrm_transport=basic
ansible_port=5986
ansible_winrm_server_cert_validation=ignore
ansible_winrm_kerberos_delegation=true